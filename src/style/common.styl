*
  padding 0
  margin 0
  list-style none
  font-style normal
  text-decoration none
  border none
  color #333
  font-weight normal
  box-sizing border-box
  &:hover
    outline none

body {
  background-color: #eee;
}
.left{
    float: left;
}

.right{
    float: right;
}

.clear:after{
    content: '';
    display: block;
    clear: both;
}

.clear{
    zoom:1;
}
